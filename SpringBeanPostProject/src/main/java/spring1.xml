<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="Selam" class="com.amine.Selam">
		<property name="mesaj" value="Bnden selam olsun"></property>
		<property name="mesaj2" value="Ahahahahahaha"></property>
	</bean>
	<bean id="Yazar" class="com.amine.Yazar" parent="Selam">
		<property name="mesaj" value="Amine Yıldız" ></property>
		<property name="mesaj3" value="Mehmet Güneş"></property>
	</bean>
</beans>
